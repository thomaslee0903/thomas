<service>
	<id>vhrm.exe</id>
	<name>VHRM</name>
	<description>Visual HRM Project</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Administrator\AppData\Roaming\npm\node_modules\qckwinsvc\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\VHRM\vhrm\app.js</argument>
	<argument>--log</argument>
	<argument>VHRM wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\VHRM\vhrm</workingdirectory>
</service>